{
  "name": "reviewdoctor-webhook-server",
  "version": "1.0.0",
  "description": "ReviewDoctor webhook receiver with Google Sheets integration",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "get-token": "tsx scripts/get-token.ts"
  },
  "keywords": [
    "reviewdoctor",
    "webhook",
    "google-sheets"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@types/node": "^25.0.3",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "googleapis": "^144.0.0",
    "lru-cache": "^10.4.3",
    "node-cron": "^4.2.1",
    "openai": "^6.15.0",
    "typescript": "^5.5.3"
  },
  "devDependencies": {
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "eslint": "^8.57.0",
    "pino-pretty": "^13.1.3",
    "prettier": "^3.3.3",
    "tsx": "^4.16.2",
    "vitest": "^1.6.0"
  }
}
